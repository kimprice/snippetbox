{
	"name": "snippetbox",
	"displayName": "SnippetBox",
	"description": "A toolbox of basic code snippets and documentation for Java",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:snippetbox.open",
		"onCommand:snippetbox.helloWorld",
		"onCommand:snippetbox.refresh",
		"onCommand:snippetbox.add",
		"onView:snippetbox-sidebar"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
			  {
				"id": "snippetbox-sidebar-view",
				"title": "SnippetBox",
				"icon": "media/book.svg"
			  }
			]
		  },
		  "views": {
			"snippetbox-sidebar-view": [
			  {
				"type": "webview",
				"id": "snippetbox-sidebar",
				"name": "SnippetBox",
				"icon": "media/book.svg",
				"contextualTitle": "SnippetBox"
			  }
			]
		  },
		"commands": [
			{
				"command": "snippetbox.open",
				"category": "Adaptive Tool",
				"title": "SnippetBox"
			},
			{
				"command": "snippetbox.helloWorld",
				"category": "Adaptive Tool",
				"title": "HelloWorld"
			},
			{
				"command": "snippetbox.refresh",
				"category": "Adaptive Tool",
				"title": "refresh"
			},
			{
				"command": "snippetbox.add",
				"category": "Adaptive Tool",
				"title": "Add Todo From Selection"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "rollup -c && tsc -p ./",
		"watch": "concurrently \"rollup -c -w\" \"tsc -watch -p ./\"",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^19.0.0",
		"@rollup/plugin-node-resolve": "^13.0.0",
		"@rollup/plugin-typescript": "^8.2.1",
		"@tsconfig/svelte": "^2.0.1",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"concurrently": "^6.2.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"rollup": "^2.51.2",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-svelte": "^6.1.1",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.38.2",
		"svelte-check": "^2.1.0",
		"svelte-preprocess": "^4.7.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	}
}
